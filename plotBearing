Gauss1(x,mu,sigma) = 1.0/(sigma*sqrt(2*pi)) * exp( -(x-mu)**2 / (2*sigma**2) )
Gauss2(x,mu,sigma) = 0.75/(sigma*sqrt(2*pi)) * exp( -(x-mu)**2 / (2*sigma**2) )
Gauss3(x,mu,sigma) = 0.5/(sigma*sqrt(2*pi)) * exp( -(x-mu)**2 / (2*sigma**2) )
Gauss4(x,mu,sigma) = 0.25/(sigma*sqrt(2*pi)) * exp( -(x-mu)**2 / (2*sigma**2) )
